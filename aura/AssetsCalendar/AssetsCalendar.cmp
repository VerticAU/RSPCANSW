<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId,force:appHostable" extends="c:vertic_Base" access="global">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="recordId" type="String" />
    <aura:attribute
        name="AssetsCalendar"
        type="B25__Reservation__c"
    />
    <aura:attribute
        name="myFilters"
        type="Object[]"
        default="[{'fieldName':'B25__Parent__c', 'operator':'=', 'value':'a301y0000001Pa2AAE'}]"
    />
    <B25:multiCalendarWrapper
        calendarId="Resources"
        view="day"
        startDate="{!v.meta.dto.deliveryDate}"
        hiddenDimensionFilters="{!v.myFilters}"
    >
    </B25:multiCalendarWrapper>
</aura:component>